{
    "json.schemas": [
        {
            "fileMatch": [
                "/lang/*.json"
            ],
            "url": "./.vscode/probe.lang-schema.json"
        },
        {
            "fileMatch": [
                "/probe/docs/*.json"
            ],
            "url": "./.vscode/probe.doc-schema.json"
        }
    ],
    "files.exclude": {
        "kubejs/probe/": true,
        "backups/": true,
        "cache/": true,
        "coremods/": true,
        "data/": true,
        "defaultconfigs/": true,
        "journeymap/": true,
        "local/": true,
        "modernfix/": true,
        "debug/": true,
        "server-resource-packs/": true
    }
}